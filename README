	test1 output:
function emptyFunc() {
	var x = 5 + 2 ;
}
function func(var x ) {
	x = x + 2 ;
	x = x / 2 ;
	function nestedFunc(var x ) {
	x = x - 2 ;
	return x ;
}
	return nestedFunc(x ) ;
}
function twoArgs(var x , var y ) {
	x = x * y ;
	return x + y ;
}
var a = 2.100000 ;
var globVar = 9 ;
var globVar2 ;
function main() {
	var a = 2 ;
	var b = 100 ;
	if(a < b ){
	a = a * 2 ;
}else if(a > 10 ){
	b = b + 10 ;
}else {
	a = 0 ;
}
	a = b - a ;
	if(a == b ){
	a = a / 2 ;
}
	while(a ! 0 ){
	a = a - 1 ;
}
	emptyFunc() ;
	var returnVal = func(a ) ;
	globVar2 = twoArgs(1 ,(2 * 9 )) ;
	return 0 ;
}


